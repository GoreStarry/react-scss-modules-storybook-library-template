{
  "name": "vue-scss-modules-storybook-library-template",
  "version": "0.2.11",
  "homepage": "https://gorestarry.github.io/vue-scss-modules-storybook-library-template/",
  "author": {
    "name": "Gore Wang",
    "email": "gore.wang.f2e@gmail.com",
    "url": "https://gore.wang"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "source": "src/lib.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf docs && npm-run-all --parallel build-lib build-app build-storybook",
    "start": "vite",
    "build-lib": "rollup -c",
    "build-lib-watch": "rollup -c -w",
    "build-app": "vite build",
    "dev": "npm-run-all --parallel build-lib-watch storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./docs/storybook",
    "minor": "npm version minor --no-git-tag-version",
    "patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "vue-demi": "^0.13.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-image": "^3.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@storybook/addon-actions": "^7.0.0-beta.19",
    "@storybook/addon-essentials": "^7.0.0-beta.19",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^7.0.0-beta.19",
    "@storybook/addon-storysource": "^7.0.0-beta.19",
    "@storybook/builder-vite": "^7.0.0-beta.19",
    "@storybook/node-logger": "^7.0.0-beta.19",
    "@storybook/vue3-vite": "7.0.0-beta.19",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "npm-run-all": "^4.1.5",
    "postcss-normalize": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.9.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.56.2",
    "storybook": "^7.0.0-beta.19",
    "vue": "^3.2.45",
    "vite": "4.0.4",
    "vite-plugin-require": "^1.1.10",
    "vue-loader": "^17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
