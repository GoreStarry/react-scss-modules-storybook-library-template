{
  "name": "react-scss-modules-storybook-library-template",
  "version": "0.2.3",
  "homepage": "https://gorestarry.github.io/react-scss-modules-storybook-library-template/",
  "author": {
    "name": "Gore Wang",
    "email": "gore.wang.f2e@gmail.com",
    "url": "https://gore.wang"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "source": "src/lib.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf docs && npm-run-all --parallel build-lib build-app build-storybook",
    "start": "vite",
    "build-lib": "rollup -c",
    "build-lib-watch": "rollup -c -w",
    "build-app": "vite build && mv -f build/* docs",
    "dev": "npm-run-all --parallel build-lib-watch storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./docs/storybook",
    "minor": "npm version minor --no-git-tag-version",
    "patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.5",
    "@rollup/plugin-image": "^3.0.1",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@storybook/addon-actions": "^7.0.0-beta.8",
    "@storybook/addon-essentials": "^7.0.0-beta.8",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^7.0.0-beta.8",
    "@storybook/addon-storysource": "^7.0.0-beta.8",
    "@storybook/node-logger": "^7.0.0-beta.8",
    "@storybook/react": "^7.0.0-beta.8",
    "@storybook/react-vite": "^7.0.0-alpha.25",
    "@vitejs/plugin-react": "3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-normalize": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.7.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.56.2",
    "storybook": "^7.0.0-beta.8",
    "vite": "4.0.1",
    "vite-plugin-require": "^1.1.10"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
